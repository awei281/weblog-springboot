<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wlog.wlogadmin.mapper.log.TagMapper">

    <select id="pageTag" resultType="com.wlog.wlogadmin.model.vo.AddTagRespVO">
        select * from t_tag

        <where>
            t_tag.deleted =false
            <if test="addTagReqVO.name != null and addTagReqVO.name != ''">
               and name like concat('%',#{addTagReqVO.name},'%')
            </if>
        </where>
        ORDER BY create_time DESC
    </select>
</mapper>